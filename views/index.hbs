<table border='1' cellspacing="0" cellpadding="5">
  <thead>
    <tr>
      <th>Country</th>
      <th>Patients</th>
      <th>Watsi Funding</th>
      <th>Healthcare Spending 2012</th>
    </tr>
  </thead>
  <tbody>
    {{#each countries}}
    <tr id={{this.[0]}}>
      <td>{{this.[0]}}</td>
      <td>{{this.[1].length}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>